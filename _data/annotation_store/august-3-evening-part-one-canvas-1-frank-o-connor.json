      "motivation": [
        "supplementing",
        "commenting"
      ],
      "body": [
        {
          "type": "TextualBody",
          "value": " I maintain that that describes the 19th century novel to you. All I will say is that there isn't a parish priest in the world who wouldn't be delighted to join in an excommunication service against any modern novelist.",
          "format": "text/plain",
          "purpose": "commenting"
        },
        {
          "type": "TextualBody",
          "value": "Frank O'Connor",
          "format": "text/plain",
          "purpose": "tagging"
        },
        {
          "type": "TextualBody",
          "value": "“The Modern Novel”",
          "format": "text/plain",
          "purpose": "tagging"
        }
      ],
      "target": {
