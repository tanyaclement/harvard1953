    {
      "@context": "http://www.w3.org/ns/anno.jsonld",
      "id": "august-5-afternoon-part-four-canvas-1-william-sloane-annotation-3.json",
      "type": "Annotation",
      "motivation": [
        "supplementing",
        "commenting"
      ],
      "body": [
        {
          "type": "TextualBody",
          "value": " I assure you, the people of the United States do read novels, and I think that there is a great danger in claiming that the emperor hasn't any clothes on all the time. There really is such a thing as a novel, and people really do read them and they really are hungry for them. And the core of it, in my opinion, is what Mr. O'Connor has said in one way and what I'm trying to say in the other.",
          "format": "text/plain",
          "purpose": "commenting"
        },
        {
          "type": "TextualBody",
          "value": "William Sloane",
          "format": "text/plain",
          "purpose": "tagging"
        },
        {
          "type": "TextualBody",
          "value": "August 5 Session Discussion",
          "format": "text/plain",
          "purpose": "tagging"
        }
      ],
      "target": {
