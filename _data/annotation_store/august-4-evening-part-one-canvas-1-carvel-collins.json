  "items": [
    {
      "@context": "http://www.w3.org/ns/anno.jsonld",
      "id": "august-4-evening-part-one-canvas-1-carvel-collins-annotation-0.json",
      "type": "Annotation",
      "motivation": [
        "supplementing",
        "commenting"
      ],
      "body": [
        {
          "type": "TextualBody",
          "value": "We had expected that one of the speakers last night would be Miss Katherine Anne Porter. She, however, has been ill. We have been expecting day-by-day, as have she and her physician, that she would be well enough to come right up until this afternoon when an emissary from the conference called at her request and prepared to help her get started from New York-- from Washington here. But she-- though she was willing to come and have the show go on, in true tradition of the theater, our agent there thought that was too much of a hardship for her, that she still with a fever should not be subjected to the trip.",
          "format": "text/plain",
          "purpose": "commenting"
        },
        {
          "type": "TextualBody",
          "value": "Carvel Collins",
          "format": "text/plain",
          "purpose": "tagging"
        },
        {
          "type": "TextualBody",
          "value": "August 4 Panel Discussion",
          "format": "text/plain",
          "purpose": "tagging"
        }
      ],
      "target": {
        "source": {
          "id": "https://tanyaclement.github.io/harvard1953/august-4-evening-part-one/canvas-1/canvas",
          "type": "Canvas",
