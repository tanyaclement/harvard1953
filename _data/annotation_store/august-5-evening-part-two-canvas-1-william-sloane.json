    },
    {
      "@context": "http://www.w3.org/ns/anno.jsonld",
      "id": "august-5-evening-part-two-canvas-1-william-sloane-annotation-17.json",
      "type": "Annotation",
      "motivation": [
        "supplementing",
        "commenting"
      ],
      "body": [
        {
          "type": "TextualBody",
          "value": " And a Pulitzer Prize or awards would long ago have fallen into disrepute if it were not for the fact that a good many of them are given for various newspaper activities such as cartooning, the best news photo, and other really ridiculous subjects. I mean, imagine giving a Pulitzer Prize for a photograph and allying that with the best novel, the best play, the best work of the creative imagination.",
          "format": "text/plain",
          "purpose": "commenting"
        },
        {
          "type": "TextualBody",
          "value": "William Sloane",
          "format": "text/plain",
          "purpose": "tagging"
        },
        {
          "type": "TextualBody",
          "value": "August 5 Panel Dicussion",
          "format": "text/plain",
          "purpose": "tagging"
        }
      ],
      "target": {
        "source": {
          "id": "https://tanyaclement.github.io/harvard1953/august-5-evening-part-two/canvas-1/canvas",
          "type": "Canvas",
          "partOf": [
            {
              "id": "https://tanyaclement.github.io/harvard1953/august-5-evening-part-two/manifest.json",
              "type": "Manifest"
            }
