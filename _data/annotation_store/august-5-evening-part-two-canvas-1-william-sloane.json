    {
      "@context": "http://www.w3.org/ns/anno.jsonld",
      "id": "august-5-evening-part-two-canvas-1-william-sloane-annotation-16.json",
      "type": "Annotation",
      "motivation": [
        "supplementing",
        "commenting"
      ],
      "body": [
        {
          "type": "TextualBody",
          "value": " And let me assure you that anyone who buys advertising space in the newspaper today needs to have a good bank account. On the other hand, I'm inclined to believe that the publishers welcome in this country some competition for the Pulitzer Prize awards, which up to the time of the National Book awards, were almost the only awards which had any significance at all.",
          "format": "text/plain",
          "purpose": "commenting"
        },
        {
          "type": "TextualBody",
          "value": "William Sloane",
          "format": "text/plain",
          "purpose": "tagging"
        },
        {
          "type": "TextualBody",
          "value": "August 5 Panel Dicussion",
          "format": "text/plain",
          "purpose": "tagging"
        }
      ],
      "target": {
        "source": {
          "id": "https://tanyaclement.github.io/harvard1953/august-5-evening-part-two/canvas-1/canvas",
          "type": "Canvas",
          "partOf": [
            {
              "id": "https://tanyaclement.github.io/harvard1953/august-5-evening-part-two/manifest.json",
              "type": "Manifest"
            }
