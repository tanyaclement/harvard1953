          "type": "Canvas",
          "partOf": [
            {
              "id": "https://tanyaclement.github.io/harvard1953/august-5-afternoon-part-two/manifest.json",
              "type": "Manifest"
            }
          ]
        },
        "selector": {
          "type": "PointSelector",
          "t": "875.0"
        }
      }
    },
