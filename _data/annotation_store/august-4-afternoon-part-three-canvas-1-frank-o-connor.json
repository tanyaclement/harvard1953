      "body": [
        {
          "type": "TextualBody",
          "value": " Well, Mr. Collins, you are right on your feet.",
          "format": "text/plain",
          "purpose": "commenting"
        },
        {
          "type": "TextualBody",
          "value": "Frank O'Connor",
          "format": "text/plain",
