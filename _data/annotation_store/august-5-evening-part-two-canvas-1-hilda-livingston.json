    {
      "@context": "http://www.w3.org/ns/anno.jsonld",
      "id": "august-5-evening-part-two-canvas-1-hilda-livingston-annotation-17.json",
      "type": "Annotation",
      "motivation": [
        "supplementing",
        "commenting"
      ],
      "body": [
        {
          "type": "TextualBody",
          "value": " I haven't the vaguest idea of what the future will be. I do know that as of the moment, originals in paper bound editions have tended to be much more of the pre-fabricated book. And I'm sure this is idealism. And I know that it's very unpopular, particularly at literary conclaves.",
          "format": "text/plain",
          "purpose": "commenting"
        },
        {
          "type": "TextualBody",
          "value": "Hilda Livingston",
          "format": "text/plain",
          "purpose": "tagging"
        },
        {
          "type": "TextualBody",
          "value": "August 5 Panel Dicussion",
          "format": "text/plain",
          "purpose": "tagging"
        }
      ],
      "target": {
        "source": {
          "id": "https://tanyaclement.github.io/harvard1953/august-5-evening-part-two/canvas-1/canvas",
          "type": "Canvas",
          "partOf": [
            {
              "id": "https://tanyaclement.github.io/harvard1953/august-5-evening-part-two/manifest.json",
              "type": "Manifest"
            }
          ]
        },
        "selector": {
          "type": "PointSelector",
          "t": "1290.0"
        }
